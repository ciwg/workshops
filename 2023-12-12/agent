do
    # merge with main
    merge $agent origin/main

    # merge with other branches
    for other in *
    do
        if [ $agent == $other ]
        then
            continue
        fi
        merge $agent origin/$other/main
    done

    # make a change
    edit $agent
    sleep 10
done


