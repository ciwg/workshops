    git log --pretty=format: --name-only $lastcommit..HEAD > .changed

    # if there are no changes, then return
    if [ ! -s .changed ]
    then
        cd -
        return 0
    fi

    inputs=$(cat .changed)
    # remove blank lines
    inputs=$(echo "$inputs" | sed '/^$/d')
    # replace newlines with commas
    inputs=$(echo "$inputs" | tr '\n' ',' | sed 's/,$//')
