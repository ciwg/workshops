        agent_gitmerge -> resolveMerge [style=dashed, label="conflicts"];
    }

    agent_gitpush -> vcs [label="Agent pushes updates to VCS", weight=2];
    vcs -> agent_gitfetch [label="VCS updates are pulled by the agent"];
}

```


