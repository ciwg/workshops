        agent_gitmerge [shape=diamond, label="git merge"];
        agent_gitcommitResolve [shape=box, label="git commit (resolve)"];
        agent_gitcommitEdit [shape=box, label="git commit (edit)"];
        agent_gitpush [shape=box, label="git push"];

