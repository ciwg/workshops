    agent=$1
    cd $agent
    pwd

    # get the last commit we looked at
    if [ -f .lastcommit ]
    then
        lastcommit=$(cat .lastcommit)
    else
        # start at the beginning of time
        lastcommit=$(git rev-list --max-parents=0 HEAD)
    fi
